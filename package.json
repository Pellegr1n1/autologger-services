{
  "name": "autologger-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "typeorm:migration:create": "typeorm-ts-node-commonjs migration:create",
    "typeorm:migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "typeorm:migration:revert": "typeorm-ts-node-commonjs migration:revert",
    "typeorm:schema:sync": "typeorm-ts-node-commonjs schema:sync",
    "typeorm:schema:drop": "typeorm-ts-node-commonjs schema:drop",
    "truffle:compile": "cd infrastructure/blockchain/besu-network/contracts && truffle compile",
    "truffle:migrate": "cd infrastructure/blockchain/besu-network/contracts && truffle migrate --network besu",
    "truffle:migrate:dev": "cd infrastructure/blockchain/besu-network/contracts && truffle migrate --network development",
    "truffle:test": "cd infrastructure/blockchain/besu-network/contracts && truffle test",
    "truffle:console": "cd infrastructure/blockchain/besu-network/contracts && truffle console --network besu",
    "truffle:deploy": "npm run truffle:compile && npm run truffle:migrate",
    "blockchain:start": "./infrastructure/blockchain/besu-network/scripts/start-besu.sh",
    "blockchain:stop": "./infrastructure/blockchain/besu-network/scripts/stop-besu.sh",
    "blockchain:deploy": "./infrastructure/blockchain/besu-network/scripts/deploy-truffle.sh",
    "blockchain:redeploy": "./infrastructure/blockchain/besu-network/scripts/redeploy-contract.sh",
    "blockchain:verify": "cd infrastructure/blockchain/besu-network/contracts && truffle run verify --network besu"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.513.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/nodemailer": "^7.0.3",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "ethers": "^6.8.1",
    "fabric-ca-client": "^2.2.20",
    "fabric-network": "^2.2.20",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "winston": "^3.18.3",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@truffle/hdwallet-provider": "^2.1.15",
    "@types/bcryptjs": "^2.4.4",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "ganache-cli": "^6.12.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "solc": "^0.8.19",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "truffle": "^5.11.5",
    "truffle-plugin-verify": "^0.6.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/migrations/**",
      "!**/database/migrations/**",
      "!**/main.ts",
      "!**/database/data-source.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
